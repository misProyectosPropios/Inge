	aCollectionOfTransactions addAll: transactions ! !

!ReceptiveAccount methodsFor: 'transactions' stamp: 'Iñaki Frutos 11/4/2025 15:04:49'!
register: aTransaction
	(aTransaction isKindOf: Transferencia) ifTrue: [transferencias add: aTransaction ]
	ifFalse: [transactions add: aTransaction]
 ! !

!ReceptiveAccount methodsFor: 'transactions' stamp: 'HernanWilkinson 7/13/2011 18:37'!
transactions 

	^ transactions copy! !


!ReceptiveAccount methodsFor: 'balance' stamp: 'HAW 5/25/2019 11:24:46'!
balance

	^transactions 
		inject: 0
		into: [ :currentBalance :transaction | transaction affectBalance: currentBalance ]! !


!ReceptiveAccount methodsFor: 'testing' stamp: 'Iñaki Frutos 11/4/2025 15:03:16'!
hasRegistered: aTransaction
	(aTransaction isKindOf: Transferencia) ifTrue: [^ transferencias includes: aTransaction ].
	^ transactions includes: aTransaction 
! !

!ReceptiveAccount methodsFor: 'testing' stamp: 'HAW 5/25/2019 11:54:51'!
isComposedBy: anAccount

	^self = anAccount ! !


